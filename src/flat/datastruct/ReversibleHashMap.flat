package flat/datastruct

class ReversibleHashMap<K, V> extends HashMap<K, V> {
  HashMap<V, K> rev

  public construct() {
    rev = HashMap()
  }

  public add(K key, V value) => value {
    /*super.add(key, value)
    rev.add(value, key)*/
  }

  public getKey(V value) => rev.get(value)
  public getValue(K key) => this[key]
}